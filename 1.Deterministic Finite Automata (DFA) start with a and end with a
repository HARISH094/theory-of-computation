#include <stdio.h>
#include <string.h>
int main() {
  char input_string[MAX_LENGTH];
  int current_state = 0;

  printf("Enter a string: ");
  scanf("%s", input_string);

  int length = strlen(input_string);

  for (int i = 0; i < length; i++) {
    if (current_state == 0 && input_string[i] == 'a') {
      current_state = 1;
    } else if (current_state == 1) {
      current_state = 1;
    } else {
      current_state = 2;
    }
  }

  if (current_state == 1 && input_string[length - 1] == 'a') {
    printf("Accepted\n");
  } else {
    printf("Rejected\n");
  }

  return 0;
}
